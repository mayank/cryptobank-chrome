{
    "manifest_version": 2,
    "name": "Crypto Bank",
    "version": "1.0.0",
    "description": "Cryptocurrenies all the time",
    "browser_action": {
        "default_icon": "img/bank.png",
        "default_title": "$$ Bitcoin $$",
        "default_popup": "index.html"
    },
    "icons": {
        "128": "img/bank.png"
    },
    "background": {
        "scripts": [
            "js/jquery.min.js",
            "js/bg.js"
        ]
    },
    "permissions": [
        "notifications"
    ]
}
